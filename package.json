{
  "name": "fragments",
  "private": true,
  "version": "0.9.23",
  "description": "CCP555 fragments back-end API",
  "scripts": {
    "test:watch": "jest -c jest.config.js --runInBand --watch --",
    "test": "jest -c jest.config.js --runInBand --",
    "test:integration": "hurl --test --glob \"tests/integration/**/*.hurl\"",
    "coverage": "jest -c jest.config.js --runInBand --coverage",
    "lint": "eslint --config .eslintrc.js \"src/**\" \"tests/**\" --ignore-pattern \"tests/integration/**/*.hurl\" --ignore-pattern \"tests/integration/test_files\"",
    "start": "node src/index.js",
    "dev": "cross-env LOG_LEVEL=debug nodemon ./src/index.js --watch src",
    "debug": "cross-env LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 ./src/index.js --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://nsachivko@github.com/nsachivko/fragments.git"
  },
  "author": "Nikita Sachivko",
  "license": "UNLICENSED",
  "devDependencies": {
    "@orangeopensource/hurl": "^2.0.1",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.2",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.306.0",
    "@aws-sdk/client-s3": "^3.304.0",
    "@aws-sdk/lib-dynamodb": "^3.306.0",
    "aws-jwt-verify": "^2.1.3",
    "compression": "^1.7.4",
    "content-type": "^1.0.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-sanitizer": "^1.0.6",
    "helmet": "^6.0.1",
    "html-to-text": "^9.0.5",
    "http-auth": "^4.2.0",
    "http-auth-passport": "^1.0.7",
    "install": "^0.13.0",
    "markdown-it": "^13.0.1",
    "marked": "^4.3.0",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.6.9",
    "npm": "^9.6.4",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^8.8.0",
    "pino-colada": "^2.2.2",
    "pino-http": "^8.3.1",
    "pino-pretty": "^9.1.1",
    "sharp": "^0.32.0",
    "stoppable": "^1.1.0",
    "uuid": "^9.0.0"
  }
}
